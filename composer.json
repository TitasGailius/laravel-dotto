{
    "name": "titasgailius/laravel-dotto",
    "description": "Laravel Dotto is a very simple yet highly optimized docker setup for your Laravel application.",
    "keywords": ["laravel", "ui", "docker", "docker-compose"],
    "license": "MIT",
    "authors": [
        {
            "name": "Titas Gailius",
            "email": "titas.gailius@gmail.com"
        }
    ],
    "require": {
        "illuminate/console": "^6.0|^7.0|^8.0",
        "titasgailius/terminal": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "TitasGailius\\Dotto\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TitasGailius\\Dotto\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TitasGailius\\Dotto\\DottoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^5.2"
    }
}
